{
  "*.js": {
    "alternate": "{dirname}/__tests__/{basename}.spec.js",
    "type": "source"
  },
  "**/__tests__/*.spec.js": {
    "alternate": "{}.js",
    "type": "test"
  },
  "*.ts": {
    "alternate": "{dirname}/__tests__/{basename}.spec.ts",
    "type": "source"
  },
  "**/__tests__/*.spec.ts": {
    "alternate": "{}.ts",
    "type": "test"
  }
}
